{
  "name": "@signageos/display-linux",
  "version": "0.0.0",
  "main": "index.html",
  "files": [
    "dist",
    "README.md",
    "package.json",
    "package-lock.json"
  ],
  "repository": {
    "url": "git@gitlab.com:signageos/display-linux",
    "type": "git"
  },
  "scripts": {
    "build": "npm run build-client && npm run build-server",
    "build-client": "webpack --config=webpack.client.config.js",
    "build-server": "webpack --config=webpack.server.config.js",
    "prebuild": "npm rebuild webworker-threads",
    "postbuild": "tools/build-package.sh",
    "clean": "rm -rf dist/* && touch dist/.gitkeep",
    "develop": "npm rebuild webworker-threads && webpack --watch --config=webpack.config.js",
    "package-debug": "export DEVICE=${DEVICE-'emulator'} && ares-inspect --device $DEVICE com.lg.app.signage",
    "package-debug-remote": "export DEVICE=${DEVICE-'emulator'} && ares-inspect --device $DEVICE com.webos.app.hdmi1",
    "package-install": "export DEVICE=${DEVICE-'emulator'} && ares-install --device $DEVICE `ls dist/com.lg.app.signage_*`",
    "prepare": "npm run clean && npm run build",
    "lint": "tslint --config node_modules/hugport-codestyle/tslint.json --project tsconfig.json {src,test}/**/*.{ts,tsx}",
    "test": "NODE_ENV=test mocha-webpack --opts mocha.opts"
  },
  "author": "signageOS.io <dev@signageos.io>",
  "devDependencies": {
    "@types/async-lock": "0.0.19",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.0",
    "@types/lodash": "^4.14.116",
    "@types/mkdirp": "^0.5.2",
    "@types/mocha": "^2.2.48",
    "@types/moment-timezone": "^0.5.8",
    "@types/node": "^10.7.1",
    "@types/raven": "^1.2.1",
    "@types/raven-js": "^3.10.0",
    "@types/request": "^2.47.1",
    "@types/request-promise-native": "^1.0.15",
    "@types/sinon": "^4.3.0",
    "@types/socket.io-client": "^1.4.32",
    "@types/ws": "^6.0.0",
    "awesome-typescript-loader": "^3.4.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "copy-webpack-plugin": "^4.3.1",
    "css-loader": "^0.28.7",
    "exports-loader": "^0.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "hugport-codestyle": "0.0.10",
    "json-loader": "^0.5.7",
    "mocha": "^5.0.4",
    "mocha-webpack": "^1.0.1",
    "moment-timezone": "^0.5.21",
    "node-sass": "^4.5.3",
    "raven-js": "^3.21.0",
    "sass-loader": "^6.0.6",
    "should": "^13.2.3",
    "sinon": "^4.4.2",
    "style-loader": "^0.19.1",
    "svg-react-loader": "^0.4.5",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2",
    "webpack": "^3.11.0",
    "whatwg-fetch": "^2.0.4"
  },
  "engines": {
    "node": "^8.9.0",
    "npm": ">=5.3.0"
  },
  "engine-strict": true,
  "publishConfig": {
    "registry": "https://npm.signageos.io",
    "access": "public"
  },
  "license": "MIT",
  "dependencies": {
    "@signageos/front-display": "^3.1.0-ma-fixNodejsManagement.29279439",
    "@signageos/lib": "^1.1.1",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "lodash": "^4.17.10",
    "mkdirp": "^0.5.1",
    "raven": "^1.2.1",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5",
    "tiny-worker": "^2.1.2",
